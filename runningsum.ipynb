   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def runningSum(self, nums: List[int]) -> List[int]:\n",
    "        for i in range(1, len(nums)):\n",
    "            nums[i] += nums[i - 1]\n",
    "        return nums \n",
    "obj = Solution()\n",
    "result = obj.runningSum([1,2,3,4])\n",
    "print(result)"
   ]

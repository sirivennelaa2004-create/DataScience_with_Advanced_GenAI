    "def subtractProductAndSum(n):\n",
    "    product = 1\n",
    "    total = 0\n",
    "\n",
    "    while n > 0:\n",
    "        digit = n % 10\n",
    "        product *= digit\n",
    "        total += digit\n",
    "        n //= 10\n",
    "\n",
    "    return product - total\n",
    "\n",
    "subtractProductAndSum(234)\n"
   ]

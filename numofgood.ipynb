    "def numIdenticalPairs(nums):\n",
    "    freq = {}\n",
    "    count = 0\n",
    "\n",
    "    for num in nums:\n",
    "        if num in freq:\n",
    "            count += freq[num]\n",
    "            freq[num] += 1\n",
    "        else:\n",
    "            freq[num] = 1\n",
    "    return count\n",
    "nums = [1, 2, 3, 1, 1, 3]\n",
    "numIdenticalPairs(nums)\n"
   ]

    "def is_leap(year):\n",
    "    leap = False\n",
    "    \n",
    "    if year % 400 ==0:\n",
    "        leap = True\n",
    "    elif year % 100 ==0:\n",
    "        leap = False\n",
    "    elif year % 4 == 0:\n",
    "        leap = True\n",
    "    else:\n",
    "        leap = False\n",
    "    \n",
    "    return leap\n",
    "\n",
    "year = int(input())\n",
    "print(is_leap(year))"
   ]
